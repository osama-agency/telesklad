{
  "name": "nextadmin-nextjs",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "update-rates": "node scripts/update-exchange-rate.js",
    "demo:delivery": "npx tsx scripts/demo-delivery-tracking.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:seed": "npx ts-node prisma/seed.ts",
    "db:add-costs": "node scripts/add-prime-costs.js",
    "db:cleanup-categories": "node scripts/cleanup-categories.js",
    "postinstall": "npx prisma generate",
    "migrate:to-remote": "node scripts/migrate-data-to-remote.js",
    "migrate:structure": "node scripts/safe-migrate-structure.js",
    "db:compare": "node scripts/compare-and-sync-structure.js",
    "telegram:webhook:setup": "tsx -r tsconfig-paths/register scripts/setup-telegram-webhook.ts",
    "telegram:webhook:delete": "tsx -r tsconfig-paths/register scripts/delete-telegram-webhook.ts",
    "telegram:webhook:info": "tsx -r tsconfig-paths/register scripts/get-telegram-webhook-info.ts",
    "telegram:switch:prod": "tsx -r tsconfig-paths/register scripts/switch-bot-mode.ts production",
    "telegram:switch:test": "tsx -r tsconfig-paths/register scripts/switch-bot-mode.ts test",
    "redis:check": "tsx -r tsconfig-paths/register scripts/start-with-redis.ts check",
    "redis:demo": "tsx -r tsconfig-paths/register scripts/start-with-redis.ts demo",
    "redis:clear": "tsx -r tsconfig-paths/register scripts/start-with-redis.ts clear-cache",
    "photos:check": "tsx -r tsconfig-paths/register scripts/check-photo-migration.ts"
  },
